<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseClicked="#clickOnWallet" prefHeight="175.0" prefWidth="250.0" stylesheets="@wallet_list_menu.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vivo.controllers.WalletController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="walletName" prefHeight="100.0" prefWidth="261.0" styleClass="wallet_name" stylesheets="@wallet_list_menu.css" text="Wallet Name" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="58.0" prefWidth="124.0" styleClass="wallet_amount" stylesheets="@wallet_list_menu.css" text="Amount:" textFill="WHITE">
                     <font>
                        <Font name="Arial Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="walletAmount" layoutX="10.0" layoutY="10.0" prefHeight="58.0" prefWidth="210.0" styleClass="wallet_amount" stylesheets="@wallet_list_menu.css" text="Label" textFill="WHITE">
                     <font>
                        <Font name="Arial Bold" size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label fx:id="walletCode" prefHeight="100.0" prefWidth="261.0" styleClass="wallet_code" stylesheets="@wallet_list_menu.css" text="Code" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font></Label>
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="wallet" />
      <String fx:value="shadow" />
   </styleClass>
</AnchorPane>
